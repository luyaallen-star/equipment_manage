{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "equipment-manager",
  "version": "0.1.5",
  "identifier": "com.prelud.equipment-manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "equipment-manager",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/luyaallen-star/equipment_manage/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEI0REMwODNFRkMzODg5OTgKUldTWWlUajhQZ2pjdEFFRWNRRlQxWGJHMUh3U1Exd0hDNVdRL3BROGF4cjBFYjJwcVdyZjN0NEkK",
      "windows": {
        "installMode": "passive"
      }
    }
